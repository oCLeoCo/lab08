<!DOCTYPE html>
<html>

<head>
    <title>All Bookings</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
    <% for (var i = 1; i <= pages; i++) { %>

        [<a href="/bookings/paginate?perPage=<%= perPage %>&page=<%= i %>">
            <%= i %>
        </a>]
    
    <% } %>
    <table>
        <% for (var booking of bookings) { %>

            <tr>
                <td>
                    <%= booking.email %>
                </td>
                <td>
                    <%= booking.numTickets %>
                </td>
            </tr>

            <% } %>
    </table>
</body>

</html>